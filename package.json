{
    "name": "@threesigmaxyz/arc-ts-sdk",
    "version": "0.0.1",
    "description": "Threesigma's Starkexpress Typescript SDK client",
    "main": "dist/cmd/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "up": "npm update",
        "check": "npm outdated",
        "upgrade": "npm run up && npm run check && npm run build",
        "clean-dist": "rimraf dist/*",
        "build-esm": "tsc --project tsconfig.esm.json",
        "build-commonjs": "tsc --project tsconfig.commonjs.json",
        "build": "npm-run-all clean-dist generate-openapi build-*",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test-web": "cp ./src/packages/client/bundle.js ./examples/web/arcClientBundle.js && cp ./src/packages/crypto/bundle.js ./examples/web/cryptoUtilsBundle.js && python3 -m http.server -d ./examples/web 3001",
        "test-user": "ts-node ./examples/user/index.ts",
        "test-assets": "ts-node ./examples/assets/index.ts",
        "test-fees": "ts-node ./examples/fees/index.ts",
        "test-operations": "ts-node ./examples/operations/index.ts",
        "test-mints": "ts-node ./examples/mints/index.ts",
        "test-withdraws": "ts-node ./examples/withdraws/index.ts",
        "test-deposits": "ts-node ./examples/deposit/index.ts",
        "test-vault": "ts-node ./examples/vault/index.ts",
        "test-transfer": "ts-node ./examples/transfers/index.ts",
        "test-settlements": "ts-node ./examples/settlements/index.ts",
        "test-health": "ts-node ./examples/health/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "fmt": "npm run prettier:fix && npm run lint:fix",
        "fmt:check": "npm run prettier && npm run lint",
        "doc": "typedoc src/packages/client/index.ts --name arc --out docs/documentation/html --tsconfig tsconfig.json",
        "prepare": "husky install",
        "generate-openapi": "openapi-generator-cli generate -g typescript-axios -i swagger.json -o src/packages/client/gen && npm run fmt"
    },
    "author": "ThreeSigma <info@threesigma.xyz>",
    "license": "(MIT AND Apache-2.0)",
    "homepage": "https://github.com/threesigmaxyz/arc-ts-sdk",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/threesigmaxyz/arc-ts-sdk"
    },
    "workspaces": [
        "src/packages/client",
        "src/packages/crypto"
    ],
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "keywords": [
        "web3",
        "ts",
        "sdk",
        "arc",
        "arc"
    ],
    "files": [
        "dist",
        "bundle.js",
        "bundle.min.js"
    ],
    "dependencies": {
        "arc-client": "file:src/packages/client",
        "arc-crypto-utils": "file:src/packages/crypto",
        "assert": "^2.0.0",
        "dotenv": "^16.0.3",
        "jest-environment-jsdom": "^29.5.0",
        "tslib": "^2.5.2"
    },
    "devDependencies": {
        "@massalabs/eslint-config": "^0.0.9",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@openapitools/openapi-generator-cli": "2.7.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "chalk": "^4.1.0",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.25.5",
        "eslint-plugin-jsdoc": "^40.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "node-ts": "^5.1.2",
        "npm-run-all": "^4.1.5",
        "ora": "^5.1.0",
        "prettier": "^2.8.4",
        "prettier-eslint": "^15.0.1",
        "prettier-standard": "^15.0.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.25",
        "typescript": "^4.9.5"
    },
    "prettier": "@massalabs/prettier-config-as"
}
