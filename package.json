{
    "name": "@threesigmaxyz/starkexpress-ts-sdk",
    "version": "0.0.1",
    "description": "Threesigma's Starkexpress Typescript SDK client",
    "main": "dist/cmd/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "up": "npm update",
        "check": "npm outdated",
        "upgrade": "npm run up && npm run check && npm run build",
        "clean-dist": "rimraf dist/*",
        "build-esm": "tsc --project tsconfig.esm.json",
        "build-commonjs": "tsc --project tsconfig.commonjs.json",
        "build-bundle": "webpack",
        "build": "npm-run-all clean-dist generate-openapi build-*",
        "update-version-major": "npm version major",
        "update-version-minor": "npm version minor",
        "update-version-patch": "npm version patch",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test-user": "ts-node ./examples/user/index.ts",
        "test-assets": "ts-node ./examples/assets/index.ts",
        "test-fees": "ts-node ./examples/fees/index.ts",
        "test-operations": "ts-node ./examples/operations/index.ts",
        "test-vault": "ts-node ./examples/vault/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "fmt": "npm run prettier:fix && npm run lint:fix",
        "fmt:check": "npm run prettier && npm run lint",
        "doc": "typedoc src/index.ts --name starkexpress --out docs/documentation/html --tsconfig tsconfig.json",
        "prepare": "husky install",
        "generate-openapi": "openapi-generator-cli generate -g typescript-axios -i swagger.json -o src/gen && npm run fmt"
    },
    "author": "ThreeSigma <info@threesigma.xyz>",
    "license": "(MIT AND Apache-2.0)",
    "homepage": "https://github.com/threesigmaxyz/starkexpress-ts-sdk",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/threesigmaxyz/starkexpress-ts-sdk"
    },
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "keywords": [
        "web3",
        "ts",
        "sdk",
        "starkexpress"
    ],
    "files": [
        "dist",
        "bundle.js",
        "bundle.min.js"
    ],
    "dependencies": {
        "@metamask/eth-sig-util": "^6.0.0",
        "@starkware-industries/starkware-crypto-utils": "^0.1.1-dev.1",
        "axios": "^0.26.1",
        "bignumber.js": "^9.1.1",
        "buffer": "^6.0.3",
        "dotenv": "^16.0.3",
        "ethereumjs-util": "^7.1.5",
        "ethers": "^6.6.4",
        "ethers-utils": "^2.1.11",
        "jest-environment-jsdom": "^29.5.0",
        "js-base64": "^3.7.5",
        "string_decoder": "^1.3.0",
        "tslib": "^2.5.2",
        "util": "^0.12.5",
        "url": "^0.11.1",
        "browserify-zlib": "^0.2.0",
        "https-browserify": "^1.0.0",
        "stream-http": "^3.2.0",
        "assert": "^2.0.0",
        "crypto-browserify": "^3.12.0",
        "stream-browserify": "3.0.0"
    },
    "devDependencies": {
        "@massalabs/eslint-config": "^0.0.9",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@types/bn.js": "^5.1.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.13.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "@openapitools/openapi-generator-cli": "2.7.0",
        "chalk": "^4.1.0",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.25.5",
        "eslint-plugin-jsdoc": "^40.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "jsdom": "^21.1.1",
        "node-ts": "^5.1.2",
        "npm-run-all": "^4.1.5",
        "ora": "^5.1.0",
        "prettier": "^2.8.4",
        "prettier-eslint": "^15.0.1",
        "prettier-standard": "^16.4.1",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.25",
        "typescript": "^4.9.5",
        "webpack": "^5.82.0",
        "webpack-cli": "^5.1.1"
    },
    "prettier": "@massalabs/prettier-config-as"
}
